# Git Configuration
# Updated: 2026-02-05 19:11
# Context: undefined

[user]
    name = Developer
    email = dev@example.com

[alias]
    st = status
    co = checkout
    br = branch
    ci = commit
    lg = log --oneline --graph --decorate
    undo = reset --soft HEAD~1

[core]
    editor = vim
    autocrlf = input
    whitespace = trailing-space,space-before-tab

[push]
    default = simple
    followTags = true

[pull]
    rebase = false

[init]
    defaultBranch = main
